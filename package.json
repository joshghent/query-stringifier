{
  "name": "query-stringifier",
  "version": "1.3.1",
  "description": "A small library for build query strings",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "docs": "typedoc index.ts --target ES6 --mode file --out docs",
    "prepublish": "npm run build",
    "test": "mocha test/index.spec.ts",
    "test:coverage": "nyc --reporter=html --reporter=text mocha test/index.spec.ts"
  },
  "keywords": [
    "query",
    "string",
    "query-string",
    "stringify",
    "stringifier",
    "parse",
    "qs",
    "sql",
    "querystring",
    "executable",
    "string"
  ],
  "author": "Josh Ghent",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.1.6",
    "@types/mocha": "^5.2.5",
    "@types/node": "^12.0.2",
    "chai": "^4.2.0",
    "husky": "^3.0.0",
    "mocha": "^6.0.0",
    "nyc": "^13.0.1",
    "source-map-support": "^0.5.9",
    "ts-node": "^8.1.1",
    "typedoc": "^0.17.1",
    "typescript": "^3.1.1",
    "typings": "^2.1.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run docs && git add docs/"
    }
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "index.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  }
}
