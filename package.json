{
  "name": "query-stringifier",
  "version": "1.3.0",
  "description": "A small library for build query strings",
  "main": "dist/index.js",
  "types" : "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "docs": "jsdoc ./dist/index.js -d docs -c docs.json -R README.md",
    "test": "mocha test/index.spec.ts",
    "test:coverage": "nyc --reporter=html --reporter=text mocha test/index.spec.ts"
  },
  "keywords": [
    "query",
    "string",
    "query-string",
    "stringify",
    "stringifier",
    "parse",
    "qs",
    "sql",
    "querystring",
    "executable",
    "string"
  ],
  "author": "Josh Ghent",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.1.6",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.11.3",
    "chai": "^4.2.0",
    "husky": "^1.1.0",
    "jsdoc": "^3.5.5",
    "mocha": "^3.5.3",
    "nyc": "^13.0.1",
    "source-map-support": "^0.5.9",
    "ts-jsdoc": "^3.0.1",
    "ts-node": "^7.0.1",
    "tui-jsdoc-template": "^1.2.2",
    "typescript": "^3.1.1",
    "typings": "^2.1.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run build && npm run docs && git add docs/ dist/"
    }
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "index.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  }
}
